<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>project</display-name>
  <welcome-file-list>

    <welcome-file>index.jsp</welcome-file>
       <welcome-file>ProjectServlet</welcome-file>
    <welcome-file>TaskServelet</welcome-file>

  </welcome-file-list>

  <servlet>
    <servlet-name>TaskServelet</servlet-name>
    <servlet-class>com.gestiondeprojet.servelets.TaskServelet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>TaskServelet</servlet-name>
    <url-pattern>/tasks/*</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>ProjectServlet</servlet-name>
    <servlet-class>com.gestiondeprojet.servelets.ProjectServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ProjectServlet</servlet-name>
    <url-pattern>/projects/*</url-pattern>
  </servlet-mapping>
  
  
  
  
</web-app>